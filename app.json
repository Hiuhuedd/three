    "name": "360",
    "slug": "student",
    "version": "2.2.0",
    "scheme": "com.threesixty.student",
    "orientation": "portrait",
    "icon": "./assets/360.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/360splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      ["expo-document-picker","expo-image-picker"]
    ],
    "android": {
      "versionCode":4,
      "package": "com.threesixty.student",
      "adaptiveIcon": {
        "foregroundImage": "./assets/360.png",
        "backgroundColor": "#ffffff"
      }

    },
    "notification": {
      "icon": "./assets/notificationIcon.png",
      "color": "#111111" 
    },
    
    "web": {
      "favicon": "./assets/360.png"
    },
    "extra": {
      "eas": {
        "projectId": "060a032f-4700-451b-9f8a-4c57b926e7ab"
      }
    }
  }
